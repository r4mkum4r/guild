<nav id="primary-nav-desktop" class="primary-nav">
    <ul>
        {{#with menus.bookMenuId}}
            <li class="menu-item menu-item-has-children">
                <a id="primary-nav-book">{{label}}</a>
                <ul id="{{id}}" class="submenu list">
                    <div class="submenu-group">
                    {{#each items}}
                            <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" class="medium">{{this.label}}</a></li>
                    {{/each}}
                    </div>
                </ul>
            </li>
        {{/with}}

        {{#with menus.tripMenuId}}
            <li class="menu-item">
                <a id="primary-nav-trips" id="{{id}}" href="{{url}}">{{label}}</a>
            </li>
        {{/with}}

        {{#if menus.expense}}
            <li class="menu-item">
                <a id="primary-nav-expense" href="/expense">{{i18nMsg "Expense"}} <span class="badge badge-info h6 sub-header">{{i18nMsg "New"}}</span></a>
            </li>
        {{/if}}

        {{#with menus.toolMenuId}}
            <li class="menu-item menu-item-has-children">
                <a id="primary-nav-tools">{{label}}</a>
                <div id="{{id}}" class="submenu multi-list">
                    {{#with items}}
                        <div class="submenu-group">
                            {{#if generalMenuId}}
                                <h4 class="category-title">{{generalMenuId.label}}</h4>
                                <ul>
                                    {{#each generalMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                            {{#if expenseMenuId}}
                                <h4 class="category-title">{{expenseMenuId.label}}</h4>
                                <ul>
                                    {{#each expenseMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                            {{#if travMgmtMenuId}}
                                <h4 class="category-title">{{travMgmtMenuId.label}}</h4>
                                <ul>
                                    {{#each travMgmtMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                        </div>
                        <div class="submenu-group">
                            {{#if travPolicyMenuId}}
                                <h4 class="category-title">{{travPolicyMenuId.label}}</h4>
                                <ul>
                                    {{#each travPolicyMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                            {{#if userMgmtMenuId}}
                                <h4 class="category-title">{{userMgmtMenuId.label}}</h4>
                                <ul>
                                    {{#each userMgmtMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                        </div>
                        <div class="submenu-group">
                            {{#if compMgmtMenuId}}
                                <h4 class="category-title">{{compMgmtMenuId.label}}</h4>
                                <ul>
                                    {{#each compMgmtMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                            {{#if reportingMenuId}}
                                <h4 class="category-title">{{reportingMenuId.label}}</h4>
                                <ul>
                                    {{#each reportingMenuId.items}}
                                        <li class="menu-item"><a id="{{this.id}}" href="{{this.url}}" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a></li>
                                    {{/each}}
                                </ul>
                            {{/if}}
                        </div>
                    {{/with}}
                </div>
            </li>
        {{/with}}

        <li class="menu-item">
            <a id="primary-nav-feedback" href="#">{{i18nMsg "uitk_feedback"}}</a>
        </li>

        {{#if menus.adminMenuId}}
            {{#with menus.adminMenuId}}
                <li class="menu-item menu-item-has-children">
                    <a id="primary-nav-admin" class="agent-only">{{label}}</a>
                    <ul id="{{id}}" class="submenu list">
                        <div class="submenu-group">
                        {{#each items}}
                                <li class="menu-item">
                                    <a id="{{this.id}}" href="{{this.url}}" class="medium" {{#if this.opensNewWindow}}target=“_blank”{{/if}}>{{this.label}}</a>
                                </li>
                        {{/each}}
                        </div>
                    </ul>
                </li>
            {{/with}}
        {{/if}}
    </ul>
</nav>